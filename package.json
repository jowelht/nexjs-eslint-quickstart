{
    "name": "eslint-setup",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "yarn format & yarn lint --fix",
        "export": "next build && next export",
        "format": "prettier --ignore-path .prettierignore --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "clean": "rimraf *.tsbuildinfo .next out yarn-error.log",
        "clean-all": "rimraf *.tsbuildinfo node_modules .next out yarn-error.log",
        "prepare": "husky install",
        "pre-commit": "yarn lint"
    },
    "dependencies": {
        "autoprefixer": "10.4.14",
        "next": "13.4.4",
        "postcss": "8.4.24",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "tailwindcss": "3.3.2"
    },
    "devDependencies": {
        "eslint": "^8.42.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-next": "^13.4.4",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.2",
        "prettier": "^2.8.8"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
        "*": "prettier --write --ignore-unknown"
    }
}
